{
    "private": true,
    "scripts": {
        "dev": "npm run development --section=front && npm run development --section=back",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "dev-front": "npm run development --section=front",
        "dev-back": "npm run development --section=back",
        "watch": "npm run development -- --watch",
        "watch-front": "npm run watch --section=front --watch",
        "watch-back": "npm run watch --section=back --watch",
        "watch-poll-front": "npm run watch --section=front --watch-poll",
        "watch-poll-back": "npm run watch --section=back --watch-poll",
        "prod": "npm run production --section=front && npm run production --section=back",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
    },
    "devDependencies": {
        "copy-webpack-plugin": "^5.1.2",
        "cross-env": "^7.0",
        "laravel-mix": "^5.0.7",
        "laravel-mix-glob": "^1.1.4",
        "laravel-mix-merge-manifest": "^0.1.2",
        "resolve-url-loader": "^3.1.2",
        "sass": "^1.27.0",
        "sass-loader": "^7.3.1",
        "vue-template-compiler": "^2.6.12",
        "webpack": "^4.44.2",
        "webpack-cli": "^3.3.12"
    },
    "dependencies": {
        "jquery": "^3.5.1",
        "jquery-validation": "^1.19.2",
        "js-cookie": "^2.2.1"
    }
}
